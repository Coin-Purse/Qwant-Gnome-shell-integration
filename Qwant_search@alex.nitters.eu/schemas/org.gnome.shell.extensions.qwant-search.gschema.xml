<schemalist gettext-domain="Qwant_search@alex.nitters.eu">
  <enum id="org.gnome.shell.extensions.Qwant_search@alex.nitters.eu.category">
    <value value="0" nick="all"/>
    <value value="1" nick="web"/>
    <value value="2" nick="news"/>
    <value value="3" nick="social"/>
    <value value="4" nick="images"/>
    <value value="5" nick="videos"/>
    <value value="6" nick="shopping"/>
    <value value="7" nick="music"/>
  </enum>
  <schema
    id="org.gnome.shell.extensions.Qwant_search@alex.nitters.eu"
    path="/org/gnome/shell/extensions/Qwant_search@alex.nitters.eu/">
